{
  "indentWidth": 2,
  "newLineKind": "lf",
  "useTabs": false,
  "typescript": {
    "lineWidth": 80,
    "semiColons": "prefer",
    "quoteStyle": "preferSingle",
    "quoteProps": "asNeeded",
    "trailingCommas": "never",
    "module.sortImportDeclarations": "maintain"
  },
  "json": {},
  "includes": [
    "**/*.{ts,tsx,js,jsx,cjs,mjs,json}"
  ],
  "excludes": [
    "**/dist",
    "**/node_modules",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.84.4.wasm",
    "https://plugins.dprint.dev/json-0.17.2.wasm"
  ]
}
