    "@vitejs/plugin-vue": "^2.3.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "unocss": "^0.39.3",
    "unplugin-icons": "^0.14.5",
    "vite": "^2.9.12",
    "vitest": "^0.15.2",
    "vue-tsc": "^0.38.1"
  },
  "packageManager": "pnpm@7.3.0"
}
